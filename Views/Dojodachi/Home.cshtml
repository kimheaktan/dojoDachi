@model Dachi
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Your pet is waiting!</h1>
    @* <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p> *@
</div>

<div class="text-center">
    <div>Fullness: @Model.Fullness </div>
    <div>Happiness: @Model.Happiness</div>
    <div>Meals: @Model.Meals</div>
    <div>Energy: @Model.Energy</div>
</div>
<div class="d-flex justify-content-around" style="margin: 0 130px 0 130px">
    <img  src="~/images/straight.gif" alt="">
</div>
    
<div class="d-flex justify-content-around mt-4">
    <partial name="WorkPartial"/>
    <partial name="FeedPartial"/>
    <partial name="PlayPartial"/>
    <partial name="SleepPartial"/>
</div>


