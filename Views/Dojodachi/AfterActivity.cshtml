@model Dachi
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Your pet is cute!</h1>
    @* <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p> *@
</div>

<div class="text-center">
    <div>Fullness: @Model.Fullness  </div> 
    <div>Happiness: @Model.Happiness</div>
    <div>Meals:@Model.Meals </div>
    <div>Energy: @Model.Energy</div> 
